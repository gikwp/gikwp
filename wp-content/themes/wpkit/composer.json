{
  "name": "gikwp/wpkit",
  "description": "WordPress starter theme with a modern development workflow.",
  "type": "wordpress-theme",
  "keywords": [
    "wordpress",
    "wordpress-theme"
  ],
  "support": {
    "issues": "https://github.com/gikwp/wpkit/issues"
  },
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.7.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Wpkit\\": "app/"
    }
  },
  "authors": [
    {
      "name": "Garegin Hakobyan",
      "email": "garik.28h@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --extensions=php --standard=PSR12 app config"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
